{
  "name": "cm-react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for react applications with all the basic and necessary services",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p build:css start:dev",
    "start:dev": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "build:css": "node-sass-chokidar src/styles/scss -o src/styles/css --recursive --watch"
  },
  "keywords": [
    "react",
    "react",
    "boilerplate"
  ],
  "author": "Surendhar E",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.9.2",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "formik": "^2.1.3",
    "graphql": "^14.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
