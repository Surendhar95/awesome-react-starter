    "terser-webpack-plugin": "^2.3.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
